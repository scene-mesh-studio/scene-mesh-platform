{
  "name": "scene-mesh-workbench",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "clean": "rm -rf node_modules yarn.lock .next",
    "rebuild": "yarn clean && yarn install && yarn build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:print": "npx eslint --print-config eslint.config.mjs > eslint-show-config.json",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "fix:all": "npm run lint:fix && npm run fm:fix"
  },
  "dependencies": {
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "@scenemesh/entity-engine": "^0.1.49",
    "react-hook-form": "^7.58.1",
    "zod": "^3.25.67",
    "@xyflow/react": "^12.6.4",
    "@mui/material": "^7.1.2",
    "dagre": "^0.8.5",
    "@types/dagre": "^0.7.52",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.8",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "elkjs": "^0.10.0",
    "@mantine/core": "8.2.5",
    "@mantine/hooks": "8.2.5",
    "@mantine/modals": "8.2.5",
    "@mantine/notifications": "8.2.5",
    "mantine-datatable": "8.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}